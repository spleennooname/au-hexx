{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "quotes": [1, "double"],
        "no-unused-vars":[1],
        "no-loss-of-precision":[1]
    }
}
